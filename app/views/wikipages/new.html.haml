%h1 New Wikipage
.row-fluid
  .span2
    %ul.nav.nav-stacked.nav-tabs.sidebar
      %li= link_to "Wiki Index", wikipages_path
      %li.active= link_to "New Wikipage", new_wikipage_path
  .span10
    .form
      = form_tag '/wikipages', method: :post do
        = label_tag :name
        = text_field_tag :name
        = label_tag :content
        = text_area_tag :content, "", class: "wysihtml5"
        = submit_tag "Create", class: 'btn btn-primary btn-large'
